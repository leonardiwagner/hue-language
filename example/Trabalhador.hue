objeto Trabalhador herda Pessoa
  t Profissao
  t Empresa = "Unicef"

  andar
    imprimir "Trabalhador " + pai.Nome + " andou"

  mostrarIdade
    se eu.Idade > 18
      imprimir "Idade do trabalhador: " + pai.Idade      
    senao
      imprimir "Trabalhador menor de idade"

  setarProfissao(t profissao)
    eu.Profissao = profissao

  trabalhar(n horasDeTrabalho)
    imprimir "Trabalhador " + pai.Nome + " trabalhou " + horasDeTrabalho + " na empresa: " + eu.Profissao